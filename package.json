{
    "name": "vscode-edge-devtools",
    "displayName": "%displayName%",
    "description": "%description%",
    "version": "3.0.0",
    "license": "SEE LICENSE IN LICENSE",
    "publisher": "ms-edgedevtools",
    "preview": false,
    "icon": "icon.png",
    "extensionKind": [
        "ui"
    ],
    "author": {
        "name": "Microsoft Corporation"
    },
    "homepage": "https://github.com/Microsoft/vscode-edge-devtools/blob/main/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/Microsoft/vscode-edge-devtools"
    },
    "keywords": [
        "browser",
        "preview",
        "viewer",
        "edge",
        "webview",
        "screencast",
        "emulation"
    ],
    "engines": {
        "vscode": "^1.104.0"
    },
    "categories": [
        "Other"
    ],
    "oneDSKey": "0c6ae279ed8443289764825290e4f9e2-1a736e7c-1324-4338-be46-fc2a58ae4d14-7255",
    "main": "./out/extension",
    "sideEffects": false,
    "activationEvents": [
        "onStartupFinished",
        "onCommand:vscode-edge-devtools.launch",
        "onCommand:vscode-edge-devtools.attach",
        "onCommand:vscode-edge-devtools.newBrowserWindow",
        "onCommand:vscode-edge-devtools-view.launchHtml",
        "onCommand:vscode-edge-devtools-view.launchScreencast"
    ],
    "capabilities": {
        "untrustedWorkspaces": {
            "supported": "limited",
            "description": "%capabilities.untrustedWorkspaces.description%"
        },
        "virtualWorkspaces": false
    },
    "contributes": {
        "commands": [
            {
                "command": "vscode-edge-devtools.attach",
                "title": "%command.attach.title%",
                "category": "%category.edgeTools%"
            },
            {
                "command": "vscode-edge-devtools.launch",
                "title": "%command.launch.title%",
                "category": "%category.edgeTools%"
            },
            {
                "command": "vscode-edge-devtools-view.openSettings",
                "category": "%category.edgeTools%",
                "title": "%command.openSettings.title%"
            },
            {
                "command": "vscode-edge-devtools-view.launchHtml",
                "title": "%command.launchHtml.title%",
                "category": "%category.edgeTools%"
            },
            {
                "command": "vscode-edge-devtools-view.launchScreencast",
                "title": "%command.launchScreencast.title%",
                "category": "%category.edgeTools%"
            },
            {
                "command": "vscode-edge-devtools.newBrowserWindow",
                "title": "%command.newBrowserWindow.title%",
                "category": "%category.edgeTools%"
            },
            {
                "command": "vscode-edge-devtools.listOpenBrowsers",
                "title": "%command.listOpenBrowsers.title%",
                "category": "%category.edgeTools%"
            },
            {
                "command": "vscode-edge-devtools.switchToBrowser",
                "title": "%command.switchToBrowser.title%",
                "category": "%category.edgeTools%"
            },
            {
                "command": "vscode-edge-devtools.closeCurrentBrowser",
                "title": "%command.closeCurrentBrowser.title%",
                "category": "%category.edgeTools%"
            },
            {
                "command": "vscode-edge-devtools.navigateBrowser",
                "title": "%command.navigateBrowser.title%",
                "category": "%category.edgeTools%"
            }
        ],
        "configuration": {
            "title": "%configuration.title%",
            "type": "object",
            "properties": {
                "vscode-edge-devtools.hostname": {
                    "type": "string",
                    "default": "localhost",
                    "description": "%configuration.hostname.description%"
                },
                "vscode-edge-devtools.port": {
                    "type": "number",
                    "default": 9222,
                    "description": "%configuration.port.description%"
                },
                "vscode-edge-devtools.useHttps": {
                    "type": "boolean",
                    "default": false,
                    "description": "%configuration.useHttps.description%"
                },
                "vscode-edge-devtools.defaultUrl": {
                    "type": "string",
                    "default": "",
                    "description": "%configuration.defaultUrl.description%"
                },
                "vscode-edge-devtools.userDataDir": {
                    "type": [
                        "string",
                        "boolean"
                    ],
                    "default": true,
                    "description": "%configuration.userDataDir.description%"
                },
                "vscode-edge-devtools.headless": {
                    "type": "boolean",
                    "description": "%configuration.headless.description%",
                    "default": true
                },
                "vscode-edge-devtools.browserArgs": {
                    "type": "array",
                    "description": "%configuration.browserArgs.description%",
                    "items": {
                        "type": "string"
                    },
                    "default": []
                },
                "vscode-edge-devtools.timeout": {
                    "type": "number",
                    "description": "%configuration.timeout.description%",
                    "default": 10000
                },
                "vscode-edge-devtools.browserFlavor": {
                    "type": "string",
                    "default": "Default",
                    "enum": [
                        "Default",
                        "Stable",
                        "Beta",
                        "Dev",
                        "Canary"
                    ],
                    "enumDescriptions": [
                        "%configuration.browserFlavor.enum.default%",
                        "%configuration.browserFlavor.enum.stable%",
                        "%configuration.browserFlavor.enum.beta%",
                        "%configuration.browserFlavor.enum.dev%",
                        "%configuration.browserFlavor.enum.canary%"
                    ]
                }
            }
        },
        "menus": {
            "explorer/context": [
                {
                    "when": "resourceLangId == html",
                    "submenu": "devtoolsContextMenu",
                    "group": "navigation"
                }
            ],
            "devtoolsContextMenu": [
                {
                    "command": "vscode-edge-devtools-view.launchHtml",
                    "group": "navigation"
                },
                {
                    "command": "vscode-edge-devtools-view.launchScreencast",
                    "group": "navigation"
                }
            ]
        },
        "submenus": [
            {
                "id": "devtoolsContextMenu",
                "label": "%submenu.openWithEdge%"
            }
        ]
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "modulePathIgnorePatterns": [
            "<rootDir>/out/"
        ],
        "testPathIgnorePatterns": [
            "<rootDir>/test/harness/"
        ],
        "testRegex": "/test/.+\\.test\\.tsx?$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ],
        "globals": {
            "DEBUG": false,
            "DEVTOOLS_BASE_URI": "https://test.invalid"
        }
    },
    "scripts": {
        "package": "vsce package --out vscode-edge-devtools.vsix",
        "vscode:prepublish": "npm run build && npm run lint",
        "build": "webpack",
        "build-debug": "npm run build -- --mode development --env debug",
        "build-edge-watch": "npm run build-debug -- --env devtoolsBaseUri=http://localhost:3000/vscode_app.html",
        "build-watch": "npm run build -- --mode development && npm run watch",
        "build-and-lint": "npm run build && npm run lint",
        "watch": "npm run watch-wp",
        "watch-wp": "webpack --watch",
        "lint": "eslint -c .eslintrc.mjs .",
        "test": "npm run lint && jest --config package.json --runInBand --env=jsdom",
        "test:harness": "node --experimental-specifier-resolution=node out/test/harness/runner.js",
        "test:harness:unit": "npm run test:harness -- --suite=unit",
        "test:harness:integration": "npm run test:harness -- --suite=integration",
        "test:harness:e2e": "npm run test:harness -- --e2e",
        "test:harness:watch": "npm run test:harness -- --watch",
        "test:harness:coverage": "npm run test:harness -- --coverage",
        "test:all": "npm run test && npm run test:harness"
    },
    "dependencies": {
        "@vscode/codicons": "0.0.40",
        "@vscode/extension-telemetry": "0.9.4",
        "bufferutil": "~4.0.9",
        "lit-html": "^3.3.1",
        "puppeteer-core": "~24.23.0",
        "utf-8-validate": "~6.0.5",
        "vscode-nls": "^5.2.0",
        "ws": "~8.18.3"
    },
    "devDependencies": {
        "@types/fs-extra": "11.0.4",
        "@types/jest": "30.0.0",
        "@types/node": "24.7.0",
        "@types/vscode": "1.104.0",
        "@types/ws": "8.18.1",
        "@typescript-eslint/eslint-plugin": "8.46.0",
        "@typescript-eslint/parser": "8.46.0",
        "@vscode/vsce": "^3.6.2",
        "copy-webpack-plugin": "^13.0.1",
        "eslint": "^9.37.0",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-jsdoc": "^60.8.3",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "fs-extra": "^11.3.2",
        "jest": "^30.2.0",
        "jest-environment-jsdom": "^30.2.0",
        "ts-jest": "^29.4.4",
        "ts-loader": "^9.5.4",
        "typescript": "~5.9.3",
        "uglify-js": "^3.19.3",
        "unzipper": "^0.12.3",
        "webpack": "^5.102.1",
        "webpack-cli": "^6.0.1"
    }
}
